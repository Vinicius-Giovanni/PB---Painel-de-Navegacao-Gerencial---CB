            "name": "Current_times",
            "expression": [
              "",
              "CONCATENATE(\"Última Atualização 🕐\",FORMAT(SELECTEDVALUE(hora_din[hora]),\"HH:MM\") )"
            ],
            "lineageTag": "63b4c79a-7da9-429b-8dfb-6b4bfaffa2ef"
