          {
            "name": "username",
            "expression": [
              "",
              "VAR UsuarioCompleto = USERPRINCIPALNAME()",
              "VAR UsuarioSemDominio = LEFT(UsuarioCompleto, SEARCH(\"@\", UsuarioCompleto, 1) - 1)",
              "VAR UsuarioComEspacos = SUBSTITUTE(UsuarioSemDominio, \".\", \" \")",
              "VAR PrimeiroNome = ",
              "    LEFT(",
              "        UsuarioComEspacos,",
              "        SEARCH(\" \", UsuarioComEspacos & \" \", 1) - 1",
              "    )",
              "VAR nome = ",
              "    UPPER(LEFT(PrimeiroNome, 1)) & LOWER(MID(PrimeiroNome, 2, LEN(PrimeiroNome)))",
              "",
              "",
              "VAR hora = HOUR(NOW() - TIME(3,0,0))",
              "",
              "VAR data = ",
              "",
              "SWITCH(",
              "    TRUE(),",
              "    hora >= 5 && hora <= 12, \"☀️Bom dia, \",",
              "    hora >= 13 && hora <= 18, \"🌤️Boa tarde, \",",
              "    (hora >= 19 && hora <= 23) || (hora >= 0 && hora <= 4), \"🌙Boa noite, \"",
              ")",
              "",
              "    ",
              "",
              "RETURN data & nome"
            ],
            "lineageTag": "dbb94d45-7d26-4416-9930-4807e237749c"
          },
          {
            "name": "hora",
            "expression": "CONCATENATE(\"Horário: \",FORMAT(TIME(HOUR(NOW()),MINUTE(NOW()),0) - TIME(3,0,0),\"HH:MM\"))",
            "lineageTag": "dd911c1f-79a0-4f69-ac4f-e9cc892dd105"
          },
          {
            "name": "username2",
            "expression": [
              "",
              "VAR UsuarioCompleto = USERPRINCIPALNAME()",
              "VAR UsuarioSemDominio = LEFT(UsuarioCompleto, SEARCH(\"@\", UsuarioCompleto, 1) - 1)",
              "VAR UsuarioComEspacos = SUBSTITUTE(UsuarioSemDominio, \".\", \" \")",
              "",
              "VAR PalavrasSeparadas = SUBSTITUTE(UsuarioComEspacos, \" \", \"|\")",
              "VAR ListaPalavras = ",
              "    ADDCOLUMNS(",
              "        GENERATESERIES(1, PATHLENGTH(PalavrasSeparadas)),",
              "        \"Palavra\", PATHITEM(PalavrasSeparadas, [Value])",
              "    )",
              "VAR NomeCapitalizado = ",
              "    CONCATENATEX(",
              "        ListaPalavras,",
              "        UPPER(LEFT([Palavra], 1)) & LOWER(MID([Palavra], 2, LEN([Palavra]))),",
              "        \" \"",
              "    )",
              "RETURN NomeCapitalizado"
            ],
            "lineageTag": "5d7c72f1-2a04-4a75-90cf-4a2046bfa2b4"
          }
        ],
